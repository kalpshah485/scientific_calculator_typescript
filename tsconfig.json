{
    "compilerOptions": {
        "target": "es2017",
        "module": "ESNext",
        "outDir": "./",
        "noEmit": true,
        "strictNullChecks": true,
        "strict": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "exclude": [
        "node_modules"
    ]
}