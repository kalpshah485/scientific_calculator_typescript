{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "./",
        "noEmit": true,
        "strictNullChecks": true,
        "strict": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "exclude": [
        "node_modules"
    ]
}